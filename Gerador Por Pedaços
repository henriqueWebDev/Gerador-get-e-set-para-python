#Algoritimo gerador
vz = int(input('Quantos MÃ©todos: '))

for i in range(vz):
    print('-'*30)
    print('\n')
    x = input("Nome :" )
    print(
        "    def set{}(self, {}):\n"
        "        self.__{} = {}\n"
        "    def get{}(self):\n"
        "        return self.__{}\n"
        "    {} = property(get{}, set{})".format(x.capitalize(),x,x,x,x.capitalize(),x,x,x.capitalize(),x.capitalize())
    )
    print('\n')
